  <simulation>
  <control>
    <duration>2000</duration>
    <startmonth>1</startmonth>
    <startyear>2006</startyear>
    <decay>lazy</decay>
    <explicit_inventory>true</explicit_inventory>
  </control>

  <archetypes>
    <spec><lib>cycamore</lib><name>DeployInst</name></spec>
    <spec><lib>cycamore</lib><name>Source</name></spec>
    <spec><lib>cycamore</lib><name>Storage</name></spec>
    <spec><lib>cycamore</lib><name>Sink</name></spec>
    <spec><lib>agents</lib><name>NullInst</name></spec>
    <spec><lib>agents</lib><name>NullRegion</name></spec>
    <spec><lib>tricycle</lib><name>FusionPowerPlant</name></spec>
  </archetypes>

  <!--Start of the Commodities Section-->
  <commodity>
    <name>Tritium</name>
    <solution_priority>1.0</solution_priority>
  </commodity>
  <commodity>
    <name>Helium_3</name>
    <solution_priority>1.0</solution_priority>
  </commodity>


  <!--Start of the Recipe Section-->
  <recipe>
    <name>Tritium</name>
    <basis>atom</basis>
    <nuclide>
        <id>10030000</id>
        <comp>1</comp>
    </nuclide>
  </recipe>

  <recipe>
    <name>Helium_3</name>
    <basis>atom</basis>
    <nuclide>
        <id>20030000</id>
        <comp>1</comp>
    </nuclide>
  </recipe>

  <recipe>
    <name>Lithium</name>
    <basis>atom</basis>
    <nuclide>
      <id>30060000</id>
      <comp>0.3</comp>
    </nuclide>
    <nuclide>
      <id>30070000</id>
      <comp>0.7</comp>
    </nuclide>
  </recipe>

  <facility>
    <name>OnePlant</name>
    <config>
      <FusionPowerPlant>
        <fusion_power>100</fusion_power>
	      <TBR>1.10</TBR>
        <reserve_inventory>6.0</reserve_inventory>
        <!--<startup_inventory>8.121</startup_inventory>-->
        <sequestered_equilibrium>2.121</sequestered_equilibrium>
        <fuel_incommod>Tritium</fuel_incommod>
        <refuel_mode>schedule</refuel_mode>
        <blanket_incommod>Enriched_Lithium</blanket_incommod>
        <blanket_inrecipe>Lithium</blanket_inrecipe>
        <blanket_size>1000</blanket_size>
        <buy_quantity>0.1</buy_quantity>
        <buy_frequency>1</buy_frequency>
        <he3_outcommod>Helium_3</he3_outcommod>
        <Li7_contribution>1.0</Li7_contribution>
        <blanket_outcommod>Tritium</blanket_outcommod>
      </FusionPowerPlant>
    </config>
  </facility>

  <facility>
   <name>Storage</name>
