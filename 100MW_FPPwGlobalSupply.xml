      <Storage>
        <in_commods>
            <val>Tritium</val>
        </in_commods>
        <out_commods>
            <val>Tritium</val>
        </out_commods>
        <in_recipe>Tritium</in_recipe>
        <sell_quantity>0.1</sell_quantity>
        <throughput>1e+299</throughput>
        <max_inv_size>200</max_inv_size>
      </Storage>
    </config>
  </facility>

